<title><%=current_user.nickname %>s Blogg </title>


<h1><%= @user%>s Blogg</h1>
    
    <% @posts.each do |post| %>
    <br />
     <br />
      

      
      <h3><%= post.title %></h3>
        <%= post.content %>
       <%= link_to 'Show', user_post_path(user_id: post.user.id,  id: post.id) %>
       <%= link_to 'Favorite', new_favorite_path %>
        <%= link_to 'Edit', edit_user_post_path(user_id: post.user.id,  id: post.id) %>
       <%= link_to 'Destroy', user_post_path(user_id: post.user.id,  id: post.id), method: :delete, data: { confirm: 'Are you sure?' } %>
      <% end %>

<br />
